<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.mailingstats %>">mailingstats</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Details of mailingstat</a></li>
</ul>

<div class="page-header">
    <h1>Details of mailingstat</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>mailing_id</th><td><%= mailingstat.mailing_id %></td></tr>

        <tr><th>email_id</th><td><%= mailingstat.email_id %></td></tr>

        <tr><th>promotion_id</th><td><%= mailingstat.promotion_id %></td></tr>

        <tr><th>email</th><td><%= mailingstat.email %></td></tr>

        <tr><th>sent_at</th><td><%= mailingstat.sent_at %></td></tr>

        <tr><th>action_id</th><td><%= mailingstat.action_id %></td></tr>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.edit_mailingstat(mailingstat), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.mailingstat(mailingstat), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.mailingstats) %>
</div>
